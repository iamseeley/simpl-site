{
  "name": "@iamseeley/simpl-site",
  "version": "1.1.6",
  "exports": {
    ".": "./mod.ts",
    "./cli": "./src/cli.ts",
    "./smallweb-simpl-site": "./src/smallweb.ts",
    "./plugin-registry": "./src/utils/PluginRegistry.ts"
  },
  "publish": {
    "include": ["mod.ts", "src/", "README.md", "LICENSE"],
    "exclude": []
  },
  "compilerOptions": {
    "allowJs": true,
    "lib": ["deno.window"],
    "strict": true
  },
  "lint": {
    "include": ["src/", "mod.ts"],
    "exclude": [],
    "rules": {
      "tags": ["recommended"]
    }
  },
  "fmt": {
    "include": ["src/", "mod.ts"],
    "exclude": [],
    "options": {
      "useTabs": false,
      "lineWidth": 80,
      "indentWidth": 2,
      "singleQuote": true,
      "proseWrap": "always"
    }
  },
  "tasks": {
    "start": "deno run --allow-read --allow-write src/cli.ts"
  },
  "imports": {
    "handlebars": "https://deno.land/x/handlebars@v0.10.0/mod.ts"
  }
}